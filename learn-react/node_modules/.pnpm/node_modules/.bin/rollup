#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/dist/bin/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/dist/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/dist/bin/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/dist/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules/rollup/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/nexus3.bilibili.co+rollup@4.40.1/node_modules:/Users/kongyajie/Desktop/workplace/learn/learn-react/demo-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
